	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="ICT13580568B.NavPage2"
    Title="Page 2"
    BackgroundColor="Green">

     <StackLayout
        Orientation="Vertical"
        VerticalOptions="Center"
        Padding="30">

        <Button
            x:Name="nextButton"
            Text="Next"
            BackgroundColor="Silver" />

        <Button
